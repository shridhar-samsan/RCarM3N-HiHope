OIL_VERSION = "2.5";

IMPLEMENTATION trampoline {

    /* This fix the default STACKSIZE of tasks to 24KB */
    TASK {
        UINT32 STACKSIZE = 0x6000 ;
    } ;

    /* This fix the default STACKSIZE of ISRs */
    ISR {
        UINT32 STACKSIZE = 4096 ;
    } ;

};

CPU app {

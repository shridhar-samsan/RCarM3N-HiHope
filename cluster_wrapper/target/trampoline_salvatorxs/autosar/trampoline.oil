    SCHEDULE = FULL;
    EVENT = maintask_sleep_event;
    EVENT = osal_event;
    RESOURCE = osal_mutex;
  };

  EVENT maintask_sleep_event {
    MASK = 1;
  };

  ALARM maintask_sleep_alarm {
    COUNTER = SystemCounter;
    ACTION = SETEVENT {
      TASK = maintask;
      EVENT = maintask_sleep_event;
    };
    AUTOSTART = FALSE;
  };


